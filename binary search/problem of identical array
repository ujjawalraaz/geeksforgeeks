#include<iostream>
#include<algorithm>
using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int arr[n],arr1[n];
	    for(int i=0;i<n;i++)
	    cin>>arr[i];
	    for(int i=0;i<n;i++)
	    cin>>arr1[i];
	    sort(arr,arr+n);
	    sort(arr1,arr1+n);
	    int k=0;
	    for(int i=0;i<n;i++)
	    {
	        if(arr[i]!=arr1[i])
	        {
	            k=1;
	            break;
	        }
	    }
	    if(k==0)
	    cout<<1<<endl;
	    else
	    cout<<0<<endl;
	}
	return 0;
}
